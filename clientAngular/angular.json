{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "clientAngular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/clientAngular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/admin-lte/plugins/fontawesome-free/css/all.min.css",
              "node_modules/admin-lte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css",
              "node_modules/admin-lte/plugins/icheck-bootstrap/icheck-bootstrap.min.css",
              "node_modules/admin-lte/plugins/jqvmap/jqvmap.min.css",
              "node_modules/admin-lte/dist/css/adminlte.min.css",
              "node_modules/admin-lte/plugins/overlayScrollbars/css/OverlayScrollbars.min.css",
              "node_modules/admin-lte/plugins/daterangepicker/daterangepicker.css",
              "node_modules/admin-lte/plugins/summernote/summernote-bs4.css",
              "src/assets/me/lib/owlcarousel/assets/owl.carousel.min.css"
            ],
            "scripts": [
              "node_modules/admin-lte/plugins/jquery/jquery.min.js",
              "node_modules/admin-lte/plugins/jquery-ui/jquery-ui.min.js",
              "node_modules/admin-lte/plugins/bootstrap/js/bootstrap.bundle.min.js",
              "node_modules/admin-lte/plugins/chart.js/Chart.min.js",
              "node_modules/admin-lte/plugins/sparklines/sparkline.js",
              "node_modules/admin-lte/plugins/jqvmap/jquery.vmap.min.js",
              "node_modules/admin-lte/plugins/jqvmap/maps/jquery.vmap.world.js",
              "node_modules/admin-lte/plugins/jquery-knob/jquery.knob.min.js",
              "node_modules/admin-lte/plugins/moment/moment.min.js",
              "node_modules/admin-lte/plugins/daterangepicker/daterangepicker.js",
              "node_modules/admin-lte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js",
              "node_modules/admin-lte/plugins/summernote/summernote-bs4.min.js",
              "node_modules/admin-lte/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js",
              "node_modules/admin-lte/plugins/fastclick/fastclick.js",
              "node_modules/admin-lte/dist/js/adminlte.js",
              "node_modules/admin-lte/dist/js/pages/dashboard.js",
              "node_modules/admin-lte/dist/js/demo.js",
              "src/assets/js/bootstrap.js",
              "src/assets/me/lib/jquery/jquery.min.js",
              "src/assets/me/lib/jquery/jquery-migrate.min.js",
              "src/assets/me/lib/popper/popper.min.js",
              "src/assets/me/lib/bootstrap/js/bootstrap.min.js",
              "src/assets/me/lib/easing/easing.min.js",
              "src/assets/me/lib/counterup/jquery.waypoints.min.js",
              "src/assets/me/lib/counterup/jquery.counterup.js",
              "src/assets/me/lib/owlcarousel/owl.carousel.min.js",
              "src/assets/me/lib/lightbox/js/lightbox.min.js",
              "src/assets/me/lib/typed/typed.min.js",
              "src/assets/me/contactform/contactform.js",
              "src/assets/me/js/main.js",
              "src/assets/clark/js/jquery.min.js",
              "src/assets/clark/js/jquery-migrate-3.0.1.min.js",
              "src/assets/clark/js/popper.min.js",
              "src/assets/clark/js/bootstrap.min.js",
              "src/assets/clark/js/jquery.easing.1.3.js",
              "src/assets/clark/js/jquery.waypoints.min.js",
              "src/assets/clark/js/jquery.stellar.min.js",
              "src/assets/clark/js/owl.carousel.min.js",
              "src/assets/clark/js/jquery.magnific-popup.min.js",
              "src/assets/clark/js/aos.js",
              "src/assets/clark/js/jquery.animateNumber.min.js",
              "src/assets/clark/js/scrollax.min.js",
              "src/assets/clark/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "clientAngular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "clientAngular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "clientAngular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "clientAngular:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "clientAngular:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "clientAngular"
}